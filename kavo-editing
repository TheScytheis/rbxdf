                    STextBox.BorderSizePixel = 0
                    STextBox.ClipsDescendants = true
                    STextBox.Position = UDim2.new(0, 0, 0.040999995, 0)
                    STextBox.Size = UDim2.new(0, 65, 0, 23)
                    STextBox.ZIndex = 99
