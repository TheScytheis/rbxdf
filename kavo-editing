                itemTextbox.RichText = true
                itemTextbox.TextColor3 = themeList.TextColor
                itemTextbox.TextSize = 14.000
                itemTextbox.TextXAlignment = Enum.TextXAlignment.Left
            

                if search == true then
                    searchframe.Name = "SearchFr"
                    searchframe.Parent = dropOpen
                    searchframe.BackgroundTransparency = 1.000
                    searchframe.BackgroundColor3 = themeList.Header
                    searchframe.Position = UDim2.new(0.900000007, 0, 0.151999995, 0)
                    searchframe.Size = UDim2.new(0, 23, 0, 23)
                    searchframe.ZIndex = 2
                    searchbtn.Name = "viewInfo"
                    searchbtn.Parent = dropOpen
                    searchbtn.BackgroundTransparency = 1.000
                    searchbtn.LayoutOrder = 9
                    searchbtn.Position = UDim2.new(0.930000007, 0, 0.22, 0)
                    searchbtn.Size = UDim2.new(0, 17, 0, 17)
                    searchbtn.ZIndex = 2
